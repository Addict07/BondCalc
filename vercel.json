{
    "builds": [
        {
            "src": "./App.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "rewrites": [
        { 
            "source": "/templates", 
            "destination": "templates/login.html" 
        }
    ]
}
